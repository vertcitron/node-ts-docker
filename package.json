{
  "name": "node-ts-docker",
  "version": "0.0.1",
  "description": "A node.js with typescript boilerplate",
  "main": "dist/app.js",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && jest",
    "dev": "ts-node-dev --respawn  --transpileOnly ./src/app.ts",
    "up:dev": "docker-compose -f docker-compose.dev.yaml up -d --force-recreate",
    "down:dev": "docker-compose -f docker-compose.dev.yaml down"
  }
}
